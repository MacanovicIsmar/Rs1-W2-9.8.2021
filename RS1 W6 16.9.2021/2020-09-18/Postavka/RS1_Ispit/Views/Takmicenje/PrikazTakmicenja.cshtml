@model RS1_Ispit_asp.net_core.Wm.TakmicenjePrikazWM


<table class="table table-bordered">

	<thead>

		<tr>

			<td>Skola</td>
			<td>razred</td>
			<td>Datum</td>
			<td>Predmet</td>
			<td>najbolji ucesnik</td>
			<td>Akcija</td>


		</tr>





	</thead>


	<tbody>


		@foreach (var a in Model.ListaTakmicenja)

		{

		<tr>
			<td>@a.Skolanaziv</td>
